{
  "manifest_version" : 2,
  "name" : "Oxpedia",
  "version" : "1.1.0",

  "browser_action" : {
    "default_popup" : "popup.html",
    "default_icon" : "icon.png"
  },
  "icons" : {
    "128" : "icon.png"
  },
  "content_scripts" : [
    {"matches" : [ "http://*/*","https://*/*" ],
    "js" : [ "penv.js" ]}
  ],

  "background" : {
    "scripts" : [ "background.js" ]
  },

  "permissions" : [
    "storage",
    "<all_urls>",
    "tabs"
  ],
  "content_security_policy" : "script-src 'self' 'unsafe-eval' https://www.google.com; object-src 'self'"
}
